parameters:
	level: max
	paths:
		- Src/
		- Tests/
	ignoreErrors:
		- # Row Transformer only returns string (contentOnly).
			path: Src/Traits/TableNodeAware.php
			identifier: generator.valueType
		- # Row Transformer not being covariant.
			path: Src/TableScraper.php
			identifier: argument.type
			count: 1
		-
			message: '|tableRowParser\(\) should return ArrayObject|'
			path: Src/TableScraper.php
			identifier: return.type
			count: 1
